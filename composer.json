{
  "name": "reason/buildsite",
  "description": "CraftCMS 5 plugin that triggers a site build via webhook",
  "type": "craft-plugin",
  "require": {
    "craftcms/cms": "^5.0",
    "guzzlehttp/guzzle": "^7.0"
  },
  "autoload": {
    "psr-4": {
      "reasondigital\\buildsite\\": "src/"
    }
  },
  "extra": {
    "class": "reasondigital\\buildsite\\Buildsite",
    "handle": "buildsite",
    "name": "Build Site"
  }
}
